import matplotlib.pyplot
import pandas as pd
from matplotlib import pyplot as plt
import seaborn as sns
from sklearn.linear_model import LinearRegression
import numpy as np


df=pd.read_csv('protein-angle-dataset.csv')
df.psi = df[["psi"]]
df.phi = df[['phi']]
df1=df[['psi','phi']]

matplotlib.pyplot.scatter(x=df.phi, y=df.psi, s=5, c='g')
plt.xlabel('Phi')
plt.ylabel('Psi')
plt.show()

matplotlib.pyplot.hist(x=df.phi, bins=50)
plt.xlabel('Phi')
plt.show()

matplotlib.pyplot.hist(x=df.psi, bins=50)
plt.xlabel('Psi')
plt.show()
